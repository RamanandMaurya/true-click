
@{
    Layout = null;
    var _memberdetails = ViewData["_memberdetails"] as webcode.Models.MemberDetailModel;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Cryptocash is Professional Creative Template" />
    <title>BAAP</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/assets/images/favicon.png">
    <link href="~/assets/css/custom.css" rel="stylesheet">

</head>
<body>
    <div class="body-wrapper">
        <section class="section-spacing login-page">
            <div class="cm-container">
                <div class="seccess-bg-images">
                    <div class="succlogin-wrapper">
                        <div class="login-wrapper-in">
                            <div class="success-page">
                                <div class="success-img-lft">
                                    <img src="~/assets/images/sucess-img1.png" alt="man" />
                                </div>
                            </div>
                            <div class="success-content-page">
                                <div class="succes-main">
                                    <div class="success-img">
                                        <img src="~/assets/images/success-check.png" alt="success" />
                                    </div>
                                    <div class="success_name">Successfull</div>
                                    <div class="succe_text-content" style="color:#808080">
                                        Congratulation @_memberdetails.Fullname, We're thrilled to have you on-board!
                                    </div>
                                    <div class="succes-tbl">
                                        <ul>
                                            <li><span class="succ-frist-d" style="color:#808080">User Name</span><span class="succ-sec-d" style="color:#808080">@_memberdetails.LoginId</span></li>
                                            <li><span class="succ-frist-d" style="color:#808080">Password</span><span class="succ-sec-d" style="color:#808080">@_memberdetails.Password</span></li>
                                            <li><span class="succ-frist-d" style="color:#808080">Email ID</span><span class="succ-sec-d" style="color:#808080">@_memberdetails.Emailid</span></li>
                                            <li><span class="succ-frist-d" style="color:#808080">Mobile No</span><span class="succ-sec-d" style="color:#808080">@_memberdetails.Mobileno</span></li>
                                            <li><a href="~/login" target="_blank" class="btn-login">Sign In</a></li>
                                        </ul>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


        </section>

    </div>
</body>
</html>