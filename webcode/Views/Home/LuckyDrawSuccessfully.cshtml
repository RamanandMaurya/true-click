@{
}

<div class="baap-lucy-drow">
  <div class="container">
    <div class="row">
      <div class="col-lg-7 col-sm-12 col-md-6">
        <div class="lucky-drow-left">
          <h1>@DateTime.Now.ToLongDateString()</h1>
          <h2><span>BAAP Network</span> Lucky Draw</h2>
          <h2 class="successfull-color">Registered Successfully</h2>
          <h3>
            You have been registered on lucky draw contest
            the unique code sent on your email address
          </h3>
          <div class="successfully-image-bird">
            <img src="~/assets/images/winner-image/8.png">
          </div>
        </div>
      </div>
      <div class="col-lg-5 col-sm-12 col-md-6">
        <div class="copon-code">
          <div class="copon-code-inner">
            <img src="~/assets/images/logo.png">
            <h1>Your Unique Code</h1>
            <h2>@ViewData["UniqueCode"].ToString()</h2>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>