                                        <th>Withdraw Status</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @{
                                        var j = 0;
                                        foreach (DataRow dr in (ViewData["_usdtreport"] as DataTable).Rows)
                                        {
                                            j++;

                                        <tr>
                                            <td>@j</td>
