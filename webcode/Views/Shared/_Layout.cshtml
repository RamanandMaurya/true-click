        <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-W57DJ4N"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->

    <div class="body-wrapper">
        <header class="custom-header">
            <div class="container">
                <nav class="custom-navbar">
                    <div class="custom-logo">
                        <a href="~/">
                            <img src="~/assets/images/logo.svg" alt="logo">
                        </a>
                    </div>

                    <ul class="login-lenu-mobile">
                        <li><a href="~/register" class="site-btn">CREATE FREE ACCOUNT</a></li>
                        <li><a href="~/login">Login</a></li>
                    </ul>

                    <div class="menu-trigger">
                        <div class="trigger-in">
                            <span></span>
                            <span></span>
                            <span></span>
                        </div>
                    </div>



                    <div class="custom-menu-wrap">
                        <ul class="custom-menu-primary">
                            <li class="menu-item active"><a href="~/">Home</a></li>
                            <li class="menu-item"><a href="~/about">About</a></li>
                            <li class="menu-item"><a href="~/products">Product</a></li>
                            <li class="menu-item"><a href="~/blog">Blog</a></li>
                            <li class="menu-item"><a href="~/contact">Contact Us</a></li>
                        </ul>
                        <ul class="login-lenu">
                            <li><a href="~/register" class="site-btn">CREATE FREE ACCOUNT</a></li>
                            <li><a href="~/login">Login</a></li>
                        </ul>
                    </div>
                </nav>
            </div>
        </header>

        @RenderBody()

        <footer class="custom-footer">

            <div class="top-footer">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-2w4">
                            <h4 class="footer_title">About</h4>
                            <div class="footer-about">
                                We laid the foundation of Best Asset Analysis People’s (BAAP) Network with an idea to bring about a change in the digital space and amalgamate the crypto... <a href="~/about">Read More</a>
                            </div>
                        </div>
                        <div class="col-lg-2w4 space-quick-link">
                            <h4 class="footer_title">Quick Links</h4>
                            <ul class="footer_link">
                                <li><a href="~/about">About Us</a></li>
                                <li><a href="~/contact">Contact Us</a></li>
                                <li><a href="~/products">Products</a></li>
                                <li><a href="~/register">Register</a></li>
                                <li><a href="~/login">Login</a></li>
                            </ul>
                        </div>
                        <div class="col-lg-2w4 space-quick-link">
                            <h4 class="footer_title">Policies</h4>
                            <ul class="footer_link">
                                <li><a href="~/disclaimer">Disclaimer</a></li>
                                <li><a href="~/privacy">Privacy Policy</a></li>
                                <li><a href="~/terms">Terms & Condition</a></li>
                            </ul>
                        </div>
                        <div class="col-lg-2w4">
                            <h4 class="footer_title">Newsletter</h4>
                            <div class="newsletter">
                                <p>Subscribe our newsletter for get Update</p>
                                <form>
                                    <div class="cm-form-group-news">
                                        <input type="email" required="required" placeholder="Email Address" class="cm-form-control" name="email">
                                        <input type="submit" class="newlette-btn site-btn" value="Send Enquery">
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="bottom_footer">
                <div class="container">
                    <div class="seprator"></div>
                    <div class="row align-items-center">
                        <div class="col-md-8">
                            <div class="copyright">
                                © @DateTime.Now.Year All Rights Reserved. Design by <a href="~/"><strong>BAAP Network</strong></a>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <ul class="footer_social_link">
                                <li><a href="https://www.facebook.com/baapnetwork" target="_blank"><i class="fa fa-facebook"></i></a></li>
                                <li><a href="https://www.instagram.com/baapnetwork/" target="_blank"><i class="fa fa-instagram"></i></a></li>
                                <li><a href="https://in.pinterest.com/baapnetwork/_created/" target="_blank"><i class="fa fa-pinterest"></i></a></li>
                                <li><a href="https://t.me/baapnetwork" target="_blank"><i class="fa fa-telegram"></i></a></li>
                                <li><a href="https://twitter.com/baapnetwork" target="_blank"><i class="fa fa-twitter"></i></a></li>
                                <li><a href="https://www.linkedin.com/company/baapnetwork" target="_blank"><i class="fa fa-linkedin"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>

        </footer>
    </div>
    <script src="~/assets/js/jquery.min.js"></script>
    <script src="~/assets/js/slick.min.js"></script>
    <script src="~/assets/js/custom.js"></script>
</body>
</html>